{
  "name": "react-sb-list-latest",
  "version": "1.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf es dist lib",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack",
    "buildx": "concurrently \"yarn build:commonjs\" \"yarn build:es\" \"yarn build:umd\" \"yarn build:umd:min\"",
    "build": "yarn clean && yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:umd:min"
  },
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  },
  "dependencies": {}
}
